<!DOCTYPE html>
<html>
<head>
  <title>Clash Royale Deck Builder</title>
  <link rel="stylesheet" type="text/css" href="css/main.css">
  <link href="https://fonts.googleapis.com/css?family=Raleway" rel="stylesheet">
</head>
<body>
  <h1>Clash Royale</h1>
    <div class="container">
    <% results.forEach(function(x) { %>
      <div class="card">
      <h2><%= x.name %></h2>
        <img class="images" src="http://www.clashapi.xyz/images/cards/<%= x.idName %>.png">
        <p>Rarity: <%= x.rarity %></p>
        <p>Type: <%= x.type %></p>
        <p><%= x.description %></p>
        <p>Arena: <%= x.arena %></p>
        <p>Elixir Cost: <%= x.elixirCost %></p>
        <input type="checkbox" name="selectcard" value="card">
    <% }) %>
    <form action="" method="get" accept-charset="utf-8">
    <input type="submit" value="Build Deck">
    </form>
    </div>
  </div>
</body>
</html>
